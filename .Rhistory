df_in$valuta[detect_hrk] <- "HRK"
df_in$valuta[detect_eur] <- "EUR"
df_in$valuta[detect_dem] <- "DEM"
df_in$valuta[!detect_hrk & !detect_eur & !detect_dem] <- NA
